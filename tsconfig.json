{
  "compilerOptions": {
      "module": "commonjs",
      "declaration": false,
      "removeComments": true,
      "emitDecoratorMetadata": true,
      "experimentalDecorators": true,
      "target": "es6",
      "sourceMap": false,
      "outDir": "./dist",
      "moduleResolution": "Node",
      "allowSyntheticDefaultImports": true,
  },
  "exclude": [
      "node_modules"
  ],
}